<launch>
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find kamu_robotu_gazebo)/worlds/kamu_map.world"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>
  <param name="robot_description" command="cat '$(find kamu_robotu_gazebo)/urdf/kamu_robotu.xacro'" />
  <include file="$(find kamu_robotu_gazebo)/launch/spawn.launch"/>
</launch>
